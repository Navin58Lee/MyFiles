package TheDuel;
public class Solution {
    static void minPower(int[] p) {
    int len = p.length;
    int sum = 0;
    int i = 0;
    for(i=1;i<100;i++)
    {
       for(int j=0;j<len;j++)
       {
    	    i = i+p[j];
       }
       if(i>0)
       {
    	   System.out.println(i);
       }
    }
    }
}
