package TheDuel;
public class Solution {
    static void minPower(int[] p) {
    int len = p.length;
    int sum = 0;
    int i = 0;
    for(i=1;i<100;i++)
    {
    	sum = i;
       for(int j=0;j<len;j++)
       {
    	    sum = sum+p[j];
       }
       if(sum>0)
       {
    	   System.out.println(sum);
    	   break;
       }
    }
    System.out.println(sum);
    }
}
