package FindTheWinner;

public class Solution {
	 static String winner(int[] andrea, int[] maria, String s) {
        int alen = andrea.length;
        int mlen = maria.length;
        int [] ares = new int[alen];
        int [] mres = new int[mlen];
        if(s == "Even")
        {
        	int x = 0;
        	for(int i=0;i<alen;i=i+2)
        	{
        		ares[x] = andrea[i] - maria[i];
        		mres[x] = maria[i] - ares[i];
        		x++;
        	}
        }
        else
        {
        	
        }

	    }

}
