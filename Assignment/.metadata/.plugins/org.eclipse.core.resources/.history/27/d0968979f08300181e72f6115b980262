import java.util.*;
public class ConsecutiveCharacters {
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		String str = in.next();
		char x = '*';
		char y;
		int len  = str.length();
		for(int i=0;i<len;i++)
		{
			int j=i+1;
			{
				if(str.charAt(j)-str.charAt(i)==1)
				{
				  y = str.charAt(j);
				  y = x;
				}
			}
		}
		System.out.println(str);
	}
}
