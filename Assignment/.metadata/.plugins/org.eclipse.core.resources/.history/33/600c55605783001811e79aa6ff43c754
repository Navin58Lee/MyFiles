		{
			num[i] = in.nextInt();
		}
        int c = num[0];
        int r = (n-1)/c;
        /*
        int arr [][] = new int[r][c];
        int k = 1;
        for(int i=1;i<=r;i++)
        {
        	for(int j=1;j<=c;j++)
        	{
        		arr[i][j] = num[k];
        		k++;
        	}
        }
        for(int i=1;i<=r;i++)
        {
        	for(int j=1;j<=c;j++)
        	{
        		System.out.println(arr[i][j]+" ");
        	}
        }   
        */
        System.out.println(c+" "+r);
	}
}
